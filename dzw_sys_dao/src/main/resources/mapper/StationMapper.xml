<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.accp.dao.StationMapper" >
  <resultMap id="BaseResultMap" type="com.accp.pojo.Station" >
    <id column="stationid" property="stationid" jdbcType="INTEGER" />
    <result column="stationname" property="stationname" jdbcType="VARCHAR" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    delete from station where stationid = #{stationid}
  </delete>
  <insert id="insertStation" parameterType="com.accp.pojo.Station" >
    insert into station (stationname)values(#{stationname})
  </insert>
  <select id="selectStationAll" resultType="com.accp.pojo.Station">
  	select * from station	
  </select>
</mapper>